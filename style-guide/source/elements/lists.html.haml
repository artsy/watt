%article.sg-component
  %h4 List Controls
  .sg-component-rendered
    .list-controls
      .list-controls-row
        .list-control-counts
          Viewing 1 - 5 of 37 Artists
        .list-control-sorting
          Sorted by: <a href="#" class="active">Most Recent</a> / <a href="#">A-Z</a>
      .list-controls-row
        .list-control-filter
          %select.form-control
            %option Filter by visibility
            %option{ value: '1' } Visible
            %option{ value: '0' } Invisible
        .list-control-filter
          %select.form-control
            %option Filter by availability
            %option{ value: '1' } Available
            %option{ value: '0' } Unavailable
  %pre
    :preserve
      .list-controls
        .list-controls-row
          .list-control-counts
            Viewing 1 - 5 of 37 Artists
          .list-control-sorting
            Sorted by: &lt;a href="#" class="active"&gt;Most Recent&lt;/a&gt; / &lt;a href="#"&gt;A-Z&lt;/a&gt;
