header {
  padding:0;
  margin:0 auto;
  width:100%;
  height:100%;
  border:none;
  z-index: 1;
  #logo-nav-session-wrap {
    display:block;
    position:fixed;
    top:0;
    left:0;
    bottom:0px;
    width:197px;
    background-color:#E6E5E6;
    border-right: 1px solid #CACACA;
    z-index: 1;
    #logo-link {
      background-position: center;
      background-repeat: no-repeat;
      height: 40px;
      margin: 15px 0 24px 24px;
      overflow:hidden;
      padding: 0;
      a {
        background:transparent url('<%= asset_url 'watt/artsy_mark_black.png' %>') top left no-repeat;
        text-indent:-999px;
        width: 40px;
        height: 40px;
        float: left;
      }
    }
  }
  #session-wrap {
  	position: absolute;
  	bottom: 0;
  	height: 170px;
    width:197px;
  	.partner-identifcation {
  	  font-family: $serif;
  	  padding: 0 0 20px 20px;
  	  margin: 0;
      font-size:16px;
      line-height:20px;
      border-bottom: 1px solid #CACACA;
      display: block;
  	}
  	.help-link {
      padding: 10px 0 0 0;
    }
  	.help-link, .sessions-link {
  	  a {
        font-size:14px;
        line-height:14px;
        padding:10px 24px;
        display: block;
        text-decoration:none;
        color: $black;
        font-weight:bold;
        text-transform: uppercase;
        font-family: $sans-serif;
  	  }
  	}
  }
}

header #logo-nav-session-wrap .navigation-links {
  padding: 0 0;
  ul {
    padding:0;
    margin:0;
    li {
      margin: 0;
      padding: 0;
      border-top: 1px solid #CACACA;
      font-size:14px;
      line-height:14px;
    }
    li:last-child {
      border-bottom: 1px solid #CACACA;
    }
  }
  a {
    padding:10px 24px;
    display: block;
    text-decoration:none;
    color: $black;
    font-weight:bold;
    text-transform: uppercase;
    font-family: $sans-serif;
  }
  li.active a {
    background-color:$white;
    color: $black;
  }
}

